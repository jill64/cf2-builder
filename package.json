{
  "name": "cf2-builder",
  "version": "1.0.4",
  "type": "module",
  "description": "üåê A toolkit to build AWS CloudFront Functions with strict type definitions",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "143883742+jill64@users.noreply.github.com",
    "url": "https://github.com/jill64",
