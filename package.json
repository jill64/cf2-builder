    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "@types/node": "20.10.4",
    "@typescript-eslint/eslint-plugin": "6.14.0",
