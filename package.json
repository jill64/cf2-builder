{
  "name": "cf2-builder",
  "version": "1.0.3",
  "type": "module",
  "description": "üåê A toolkit to build AWS CloudFront Functions with strict type definitions",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/cf2-builder/issues",
  "homepage": "https://github.com/jill64/cf2-builder#readme",
  "files": [
    "dist",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "aws",
    "cf2",
    "cloudfront",
    "functions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/cf2-builder.git"
  },
  "scripts": {
    "dev": "npx tsx src/bin.ts",
    "build": "tsc && npx publint",
    "test": "vitest",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "@types/node": "20.10.4",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint-plugin-deprecation": "2.0.0",
    "typescript": "5.3.3",
    "vitest": "1.0.3"
  },
  "dependencies": {
    "esbuild": "0.19.8"
  }
}
