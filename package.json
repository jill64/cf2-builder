{
  "name": "cf2-builder",
  "version": "0.1.2",
  "type": "module",
  "description": "A toolkit to build AWS CloudFront Functions with strict type definitions",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/cf2-builder/issues",
  "homepage": "https://github.com/jill64/cf2-builder#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "cloudfront",
    "functions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/cf2-builder.git"
  },
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx prettier --check . && npx eslint && npx depcheck",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "@types/node": "20.8.6",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "esbuild": "0.19.4"
  }
}
